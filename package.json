{
  "name": "express-typescript-app",
  "version": "1.0.0",
  "description": "Express server with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch --include './src/*' src/index.ts",
    "build": "tsc",
    "db:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:reset": "dotenv -e .env.development -- npx prisma migrate reset"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
