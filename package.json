{
  "name": "express-typescript-app",
  "version": "1.0.0",
  "description": "Express server with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "db:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:reset": "dotenv -e .env.development -- npx prisma migrate reset",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "@tsconfig/node22": "^22.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.15",
    "typescript": "^5.0.4"
  }
}
